{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "node scripts/ensure-lightningcss.mjs",
    "prepare": "husky",
    "hook:commitmsg": "echo 'npx --no -- commitlint --edit \"$1\"' > .husky/commit-msg && chmod +x .husky/commit-msg"
